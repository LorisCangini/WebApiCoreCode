<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <connectionStrings>
    <add name="SQLiteConn" 
      connectionString="Data Source=testDB.db;Version=3;" 
      providerName="System.Data.SQLite" />
    <add name="LocalSqlServConn" 
      connectionString="DataSource=(LocalDB)\MSSQLLocalDB;Initial Catalog = testDb;Integrated Security=True;Connect Timeout=20" 
      providerName="System.Data.SqlClient" />
  </connectionStrings>

  <appSettings>
    <add key="dbServer" value="SQLiteConn" />
    <!-- "SQLiteConn" "LocalSqlServConn" "RemoteSqlServConn"-->
  </appSettings>
</configuration>